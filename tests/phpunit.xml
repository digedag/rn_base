<?xml version="1.0" encoding="UTF-8"?>
<phpunit backupGlobals="false" backupStaticAttributes="true"
    convertErrorsToExceptions="true" convertNoticesToExceptions="true"
    convertWarningsToExceptions="true" processIsolation="false"
    stopOnFailure="true" syntaxCheck="true"
    testSuiteLoaderClass="PHPUnit_Runner_StandardTestSuiteLoader"
    bootstrap="Bootstrap.php"
>

<testsuites>
  <testsuite name="RNBaseTestSuite">
    <directory suffix="testcase.php">./</directory>
    <directory suffix="Test.php">./</directory>
  </testsuite>
</testsuites>
</phpunit>
